<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <Version>1.1-beta.2</Version>
        <RepositoryUrl>https://github.com/horstmeier/libprocess2</RepositoryUrl>
        <Title>Library for  running generic processes</Title>
        <PackageTags>Process</PackageTags>
        <Authors>Jens Horstmeier</Authors>
        <Copyright>(C) 2022 by Jens Horstmeier</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.1" />
    </ItemGroup>

</Project>
